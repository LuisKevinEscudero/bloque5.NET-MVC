@model prueba.Models.Movie
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>Release Date: @Model.Genre.ReleaseDate.ToShortDateString()</li>
    <li>Date Added: @Model.Genre.DateAdded.ToShortDateString()</li>
    <li>Number in Stock: @Model.Genre.NumberInStock</li>

</ul>

